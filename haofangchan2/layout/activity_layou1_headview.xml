<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/tab01"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >

        <FrameLayout
            android:id="@+id/FrameLayout"
            android:layout_width="fill_parent"
            android:layout_height="130dp" >

            <android.support.v4.view.ViewPager
                android:id="@+id/viewPager1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >
            </android.support.v4.view.ViewPager>
        </FrameLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#dedede"
            android:padding="10dp" >

            <LinearLayout
                android:id="@+id/map_view"
                android:layout_width="match_parent"
                android:layout_height="130dp"
                android:orientation="horizontal" >
            </LinearLayout>

            <LinearLayout
                android:id="@+id/fi_ll"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/map_view"
                android:layout_marginTop="10dp"
                android:background="#FFFFFF"
                android:orientation="horizontal" >
				<ImageView
                        android:id="@+id/online_watch"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:scaleType="fitXY"
                        android:src="@drawable/g_03_03" />
                <ImageView
                        android:id="@+id/house_simple_instruct"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:scaleType="fitXY"
                        android:src="@drawable/g_03_04" />

                <ImageView
                        android:id="@+id/project_magzine"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:scaleType="fitXY"
                        android:src="@drawable/g_03_05" />
            </LinearLayout>
			<LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/fi_ll"
                android:background="#FFFFFF"
                android:orientation="horizontal" >
				<ImageView
                        android:id="@+id/consultor"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:scaleType="fitXY"
                        android:layout_height="wrap_content"
                        android:src="@drawable/g_03_07" />
                <ImageView
                        android:id="@+id/calculate_pay"
                       	android:layout_width="0dp"
                        android:layout_weight="1"
                        android:scaleType="fitXY"
                        android:layout_height="wrap_content"
                        android:src="@drawable/g_03_08" />

                <ImageView
                        android:id="@+id/latestNews"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:scaleType="fitXY"
                        android:layout_height="wrap_content"
                        android:src="@drawable/g_03" />
            </LinearLayout>
            
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>