<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical" >
      <include layout="@layout/title_bar" /> 
<LinearLayout 
	           android:id="@+id/tab04"
	           android:orientation="vertical"
	           android:layout_width="match_parent"
	           android:layout_height="fill_parent">
<RelativeLayout 
               android:id="@+id/personalcenter_username"
	           android:layout_width="fill_parent"
	           android:layout_height="100dp"
	           android:background="@drawable/background1x"
	                >
<de.hdodenhof.circleimageview.CircleImageView
	           xmlns:app="http://schemas.android.com/apk/res-auto" 
	           
	           android:id="@+id/profile_image"
			   android:layout_width="60dp"
		       android:layout_height="60dp"
		       android:src="@drawable/profile_image"
			   app:border_width="0dp"
			   app:border_color="#FF000000"
			   android:layout_marginLeft="20dp"
			   android:layout_centerVertical="true" >
					</de.hdodenhof.circleimageview.CircleImageView>

			   <TextView
			   android:id="@+id/main_username"
			   android:layout_width="wrap_content"
			   android:layout_height="wrap_content"
			   android:layout_marginLeft="10dp"
			   android:layout_marginTop="20dp"
			   android:layout_toRightOf="@+id/profile_image"
			   android:textColor="#505050"
		       android:text="User_Name"
			   android:textSize="20sp" />
			   	<TextView
					    android:id="@+id/sex_personalcenter"
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_below="@+id/main_username"
					    android:layout_marginLeft="10dp"
					    android:layout_marginTop="5dp"
					    android:layout_toRightOf="@+id/profile_image"
					    android:textColor="#505050"
					    android:text="man"
					    android:textSize="10sp" />

					<TextView
					    android:id="@+id/main_gender"
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_below="@+id/main_username"
					    android:layout_marginLeft="10dp"
					    android:layout_marginTop="5dp"
					    android:layout_toRightOf="@+id/sex_personalcenter"
					    android:text="Gender_Last_Login"
					    android:textColor="#505050"
					    android:textSize="10sp" />
				

					<TextView
					    android:id="@+id/main_signature"
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_below="@+id/main_gender"
					    android:layout_marginLeft="10dp"
					    android:layout_marginTop="5dp"
					    android:layout_toRightOf="@+id/profile_image"
					    android:textColor="#505050"
					    android:text="User_Signature"
					    android:textSize="10sp" />
					<ImageView 
					    android:layout_height="30dp"
					    android:layout_width="30dp"
					    android:id="@+id/main_userprofile"
					    android:src="@drawable/whitenextstep2x"
					    android:layout_centerVertical="true"
					    android:layout_alignParentRight="true"
					    />
	            </RelativeLayout>
	            <ImageView 
	                android:layout_height="10dp"
	                android:layout_width="fill_parent"
	                android:background="#EBEBEB"/>
	    <LinearLayout
	        android:id="@+id/main_tab04second"
	         android:layout_height="100dp"
	         android:layout_width="match_parent"
	         android:orientation="horizontal"
	         >
	              <ImageView 
	               android:id="@+id/tab04image01"
	               android:layout_height="wrap_content"
	               android:layout_width="0dp"
	               android:layout_weight="1"
	               android:scaleType="fitXY"
	               android:src="@drawable/da_02"/>
	               <ImageView 
	               android:id="@+id/tab04image02"
	               android:layout_height="wrap_content"
	               android:layout_width="0dp"
	               android:layout_weight="1"
	               android:scaleType="fitXY"
	               android:src="@drawable/da_03"/>
	               <ImageView 
	                    android:id="@+id/tab04image03"
	                    android:layout_height="wrap_content"
	                  	android:layout_width="0dp"
	               		android:layout_weight="1"
	               		android:scaleType="fitXY"
	                    android:src="@drawable/da_04"/>
	                
	            </LinearLayout>
	            <ImageView 
	                android:layout_height="10dp"
	                android:layout_width="fill_parent"
	                android:background="#EBEBEB"/>
	            <ScrollView 
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content">
	            <LinearLayout
	                android:layout_width="match_parent"
	                android:layout_height="wrap_content"
	                 android:orientation="vertical"
	                >
	            <RelativeLayout 
	                android:id="@+id/magazine_personalsetting"
	                android:layout_height="50dp"
	                android:layout_width="match_parent"
	                >
	                <ImageView 
	                    android:id="@+id/main_zazhi"
	                    android:layout_width="45dp"
	                    android:layout_height="45dp"
	                    android:src="@drawable/center05"
	                    android:layout_centerVertical="true"/>
	                <TextView 
	                    android:text="杂志"
	                    android:layout_height="wrap_content"
	                    android:layout_width="wrap_content"
	                    android:layout_centerVertical="true"
	                    android:layout_toRightOf="@+id/main_zazhi"
	                    android:textSize="16sp"
	                    android:textColor="#505050"
	                    android:layout_marginLeft="10dp"/>
	                <ImageView 
	                    android:layout_width="20dp"
	                    android:layout_height="20dp"
	                    android:src="@drawable/garynextstep2x"
	                    android:layout_alignParentRight="true"
	                    android:layout_centerVertical="true"/>
	            </RelativeLayout>
	            <ImageView 
	                android:layout_height="1dp"
	                android:layout_width="fill_parent"
	                android:background="#E0E0E0"/>
	            <RelativeLayout 
	                android:id="@+id/messageCenter"
	                android:layout_height="50dp"
	                android:layout_width="match_parent"
	                >
	                <ImageView 
	                    android:id="@+id/main_friend"
	                    android:layout_width="45dp"
	                    android:layout_height="45dp"
	                    android:src="@drawable/center08"
	                    android:layout_centerVertical="true"/>
	                <TextView 
	                    android:text="消息中心"
	                    android:layout_height="wrap_content"
	                    android:layout_width="wrap_content"
	                    android:layout_centerVertical="true"
	                    android:textColor="#505050"
	                    android:textSize="16sp"
	                    android:layout_toRightOf="@+id/main_friend"
	                    android:layout_marginLeft="10dp"/>
	                <ImageView 
	                    android:layout_width="20dp"
	                    android:layout_height="20dp"
	                    android:src="@drawable/garynextstep2x"
	                    android:layout_alignParentRight="true"
	                    android:layout_centerVertical="true"/>
	            </RelativeLayout>
	            <View 
	                android:layout_height="1dp"
	                android:layout_width="fill_parent"
	                android:background="#E0E0E0"/>
	            <RelativeLayout 
	                android:id="@+id/change_sale_activityfragment4"
	                android:layout_height="50dp"
	                android:layout_width="match_parent"
	                >
	                <ImageView 
	                    android:id="@+id/main_change"
	                    android:layout_width="45dp"
	                    android:layout_height="45dp"
	                    android:src="@drawable/center10"
	                    android:layout_centerVertical="true"/>
	                <TextView 
	                    android:text="预定看房"
	                    android:layout_height="wrap_content"
	                    android:layout_width="wrap_content"
	                    android:layout_centerVertical="true"
	                    android:textColor="#505050"
	                    android:textSize="16sp"
	                    android:layout_toRightOf="@+id/main_change"
	                    android:layout_marginLeft="10dp"/>
	                <ImageView 
	                    android:layout_width="20dp"
	                    android:layout_height="20dp"
	                    android:src="@drawable/garynextstep2x"
	                    android:layout_alignParentRight="true"
	                    android:layout_centerVertical="true"/>
	            </RelativeLayout>
	            <View 
	                android:layout_height="1dp"
	                android:layout_width="fill_parent"
	                android:background="#E0E0E0"/>
	            <RelativeLayout 
	                android:id="@+id/change_saler"
	                android:layout_height="50dp"
	                android:layout_width="match_parent"
	                >
	                <ImageView 
	                    android:id="@+id/main_change1"
	                    android:layout_width="45dp"
	                    android:layout_height="45dp"
	                    android:src="@drawable/center12"
	                    android:layout_centerVertical="true"/>
	                <TextView 
	                    android:text="变换销售员"
	                    android:layout_height="wrap_content"
	                    android:layout_width="wrap_content"
	                    android:textColor="#505050"
	                    android:layout_centerVertical="true"
	                    android:textSize="16sp"
	                    android:layout_toRightOf="@+id/main_change1"
	                    android:layout_marginLeft="10dp"/>
	                <ImageView 
	                    android:layout_width="20dp"
	                    android:layout_height="20dp"
	                    android:src="@drawable/garynextstep2x"
	                    android:layout_alignParentRight="true"
	                    android:layout_centerVertical="true"/>
	            </RelativeLayout>
	            <View 
	                android:layout_height="10dp"
	                android:layout_width="fill_parent"
	                android:background="#EBEBEB"/>

	            <RelativeLayout
	                android:id="@+id/setting_personalcenter"
	                android:layout_width="match_parent"
	                android:layout_height="50dp"
	                android:clickable="true" >

	                <ImageView
	                    android:id="@+id/main_setting"
	                    android:layout_width="45dp"
	                    android:layout_height="45dp"
	                    android:layout_centerVertical="true"
	                    android:src="@drawable/center14" />

	                <TextView
	                    android:id="@+id/textView1"
	                    android:layout_width="wrap_content"
	                    android:layout_height="wrap_content"
	                    android:layout_centerVertical="true"
	                    android:layout_marginLeft="10dp"
	                    android:layout_toRightOf="@+id/main_setting"
	                    android:clickable="true"
	                    android:text="设置"
	                    android:textSize="16sp"
	                    android:textColor="#505050" />

	                <ImageView
	                    android:layout_width="20dp"
	                    android:layout_height="20dp"
	                    android:layout_alignBottom="@+id/textView1"
	                    android:layout_alignParentRight="true"
	                    android:src="@drawable/garynextstep2x" />
	            </RelativeLayout>

	            <View 
	                android:layout_height="10dp"
	                android:layout_width="fill_parent"
	                android:background="#EBEBEB"/>
	            </LinearLayout>
	            </ScrollView>
	            <ImageView 
	                android:layout_height="match_parent"
	                android:layout_width="fill_parent"
	                android:background="#EBEBEB"/>
	        </LinearLayout>
</LinearLayout>
